{
  "metadata": [ ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      { "files": ["**/*.md", "**/toc.yml"], "src": "articles", "dest": "docs" },
      { "files": [ "toc.yml", "*.md" ] }
    ],
    "resource": [
      {
        "files": [ "**/images/**", "**/resources/**" ],
        "exclude": [ "_site/**", "obj/**" ]
      }
    ],
    "output": "_site",
    "globalMetadata": {
      "_appTitle": "Multi-platform mod installer",
      "_appName": "MOXMI",
      "_appFooter": "<span>Copyright (c) 2019 Benito Palacios SÃ¡nchez. Docs made with <a href=\"https://dotnet.github.io/docfx\" target=\"_blank\">docfx</a></span>",
      "_appLogoPath": "images/logo_48.png",
      "_appFaviconPath": "images/favicon.png",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_lang": "en"
    },
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern",
      "template"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false,
    "sitemap": {
      "baseUrl": "https://pleonex.dev/game-patcher",
      "priority": 0.5,
      "changefreq": "monthly"
    }
  }
}